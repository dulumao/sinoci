{
  "name": "sinoci/sinoci",
  "description": "Simple yet powerful framework for building modern web sites.",
  "version": "0.3.1",
  "homepage": "https://github.com/sinoci/sinoci",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/sinoci/sinoci/issues",
    "wiki": "https://github.com/sinoci/sinoci/wiki"
  },
  "require": {
    "php": ">=5.6.0",
    "codeigniter/framework": "3.1.*",
    "guzzlehttp/guzzle": "6.2.*",
    "illuminate/database": "5.3.*",
    "illuminate/pagination": "5.3.*",
    "illuminate/view": "5.3.*",
    "leafo/scssphp": "0.6.*"
  },
  "require-dev": {
    "psy/psysh": "0.8.*",
    "sami/sami": "4.0.*",
    "uikit/uikit": "2.27.*",
    "workerman/phpsocket.io": "1.1.*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "files": [
      "resources/helpers/functions.php"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  },
  "config": {
    "process-timeout": 0
  },
  "scripts": {
    "run": "App\\Services\\Script::run"
  }
}
